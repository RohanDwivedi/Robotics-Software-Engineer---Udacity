<?xml version='1.0'?>

<robot name='my_robot' xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />

	<gazebo reference='robot_base'>
		<material> Gazebo/Grey </material>
	</gazebo>

	<gazebo reference='LeftFrontWheel'>
		<material> Gazebo/FlatBlack </material>
	</gazebo>

	<gazebo reference='RightFrontWheel'>
		<material> Gazebo/FlatBlack </material>
	</gazebo>

	<gazebo reference='LeftBackWheel'>
		<material> Gazebo/FlatBlack </material>
	</gazebo>

	<gazebo reference='RightBackWheel'>
		<material> Gazebo/FlatBlack </material>
	</gazebo>

	<link name="robot_footprint"></link>

  	<joint name="robot_footprint_joint" type="fixed">
    		<origin xyz="0 0 0" rpy="0 0 0" />
    		<parent link="robot_footprint"/>
   		<child link="robot_base" />
  	</joint>

	<link name="robot_base">
    
	<visual name = 'robot_base_visual'>
        	<origin xyz = '0 0 0' rpy = '0 0 0' />
        	<geometry> <box size='2 0.6 0.3'/> </geometry>
   	</visual>
	
	<visual name = 'robot_LidarPlate'>
        	<origin xyz = '0 0 0.15' rpy = '0 0 0' />
        	<geometry> <box size='0.2 0.2 0.35'/> </geometry>
   	</visual>
  
   	<visual name = 'robot_CameraPlate'>
        	<origin xyz = '0.8 0 0.15' rpy = '0 0 0' />
        	<geometry> <box size='0.2 0.2 0.025'/> </geometry>
   	</visual>
    
        
    	<collision name = 'collision' >
        	<origin xyz = '0 0 0' rpy = '0 0 0' />  
        	<geometry> <box size='2 0.6 0.3'/> </geometry>
    	</collision>
        
    	<inertial>
        	<origin xyz = '0 0 0' rpy='0 0 0' />
        	<mass value='15'/>
        	<inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    	</inertial>
        
    </link>
     
    <link name="LeftFrontWheel">
     
    	<visual name='LeftFrontWheel_visual'>
        	<origin xyz = '0 0 0' rpy = '0 1.5707 1.5707' />
        	<geometry> <cylinder radius='0.3' length='0.07'/> </geometry>
    	</visual>
        
        <collision name = 'collision'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.3' length='0.07'/> </geometry>
        </collision>
        
        <inertial>
        	<origin xyz = '0 0 0' rpy='0 0 0' />
        	<mass value='5' />
        	<inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
        </inertial>
     
    </link>    

    <link name="RightFrontWheel">
     
    	<visual name='RightFrontWheel_visual'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.3' length='0.07'/> </geometry>
    	</visual>
        
        <collision name = 'collision'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.3' length='0.07'/> </geometry>
        </collision>
        
        <inertial>
        	<origin xyz = '0 0 0' rpy='0 0 0' />
        	<mass value='5' />
        	<inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
        </inertial>
     
    </link>    
    
    <link name="LeftBackWheel">
     
    	<visual name='LeftBackWheel_visual'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.4' length='0.07'/> </geometry>
    	</visual>
        
        <collision name = 'collision'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.4' length='0.07'/> </geometry>
        </collision>
        
        <inertial>
        	<origin xyz = '0 0 0' rpy='0 0 0' />
        	<mass value='5' />
        	<inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
        </inertial>
     
    </link>    

    <link name="RightBackWheel">
     
    	<visual name='RightBackWheel_visual'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.4' length='0.07'/> </geometry>
    	</visual>
        
        <collision name = 'collision'>
        	<origin xyz = '0 0 0' rpy = '1.5707 1.5707 0' />
        	<geometry> <cylinder radius='0.4' length='0.07'/> </geometry>
        </collision>
        
        <inertial>
        	<origin xyz = '0 0 0' rpy='0 0 0' />
        	<mass value='5' />
        	<inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
        </inertial>
     
    </link>    

    <link name='camera'>
	
	<visual name='camera_visual'>
		<origin xyz ='0 0 0' rpy='0 0 0' />
		<geometry> <box size='0.05 .05 .05'/> </geometry>
	</visual>

	<collision name = 'collision'>
		<origin xyz ='0 0 0' rpy='0 0 0' />
		<geometry> <box size='0.05 0.05 0.05'/> </geometry>
	</collision>

	<inertial>
		<origin xyz='0.05 0.05 0.05' rpy = '0 0 0' />
		<mass value='0.1'/>
		<inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
        </inertial>

    </link>	
	
    <link name='hokuyo'>
	
	<visual name='hokuyo_visual'>
		<origin xyz ='0 0 0' rpy='0 0 0' />
		<geometry> <mesh filename="package://my_robot/meshes/hokuyo.dae"/> </geometry>
	</visual>

	<collision name = 'collision'>
		<origin xyz ='0 0 0' rpy='0 0 0' />
		<geometry> <box size='0.1 0.1 0.1'/> </geometry>
	</collision>

	<inertial>
		<origin xyz='0.0 0.0 0.0' rpy = '0 0 0' />
		<mass value='1e-5'/>
		<inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
        </inertial>

    </link>		
	

    <joint name="joint_LeftFrontWheel" type="revolute">
    	<origin xyz='0.8 0.35 0' rpy='0 0 0' />
        <parent link='robot_base'/>
        <child link='LeftFrontWheel'/>
	<limit lower='-3.14' upper='3.14' effort='10' velocity='5'/>
        <axis xyz='0 -1 0'/>
    </joint>

    <joint name="joint_RightFrontWheel" type="revolute">
    	<origin xyz='0.8 -0.35 0' rpy='0 0 0' />
        <parent link='robot_base'/>
        <child link='RightFrontWheel'/>
	<limit lower='-3.14' upper='3.14' effort='10' velocity='5'/>
        <axis xyz='0 1 0'/>
    </joint>

    <joint name="joint_LeftBackWheel" type="continuous">
    	<origin xyz='-0.8 0.35 0' rpy='0 0 0' />
        <parent link='robot_base'/>
        <child link='LeftBackWheel'/>
		<limit effort="10000" velocity="1000"/>
    	<dynamics damping="1.0" friction="1.0"/>
        <axis xyz='0 -1 0'/>
    </joint>

    <joint name="joint_RightBackWheel" type="continuous">
    	<origin xyz='-0.8 -0.35 0' rpy='0 0 0' />
        <parent link='robot_base'/>
        <child link='RightBackWheel'/>
	    <limit effort="10000" velocity="1000"/>
        <dynamics damping="1.0" friction="1.0"/>
        <axis xyz='0 1 0'/>
    </joint>

    <joint name ='camera_joint' type = 'fixed'>
	<origin xyz='0.8 0 0.19' rpy='0 0 0' />
	<parent link='robot_base'/>
	<child link='camera' />
	<axis xyz='0 1 0' />
    </joint>	

    <joint name ='hokuyo_joint' type = 'fixed'>
	<origin xyz='0 0 0.35' rpy='0 0 0' />
	<parent link='robot_base'/>
	<child link='hokuyo' />
	<axis xyz='0 1 0' />
    </joint>	

</robot>
        
